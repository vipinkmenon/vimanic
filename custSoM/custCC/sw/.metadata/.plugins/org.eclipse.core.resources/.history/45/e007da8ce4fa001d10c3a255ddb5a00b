


//Frequency BIT masks

#define mask_0_001 0x40
#define mask_0_002 0x80
#define mask_0_004 0x100
#define mask_0_008 0x200
#define mask_0_01 0x400
#define mask_0_02 0x800
#define mask_0_04 0x1000
#define mask_0_08 0x2000
#define mask_0_1 0x4000
#define mask_0_2 0x8000
#define mask_0_4 0x10000
#define mask_0_8 0x20000
#define mask_1 0x40000
#define mask_2 0x80000
#define mask_4 0x100000
#define mask_8 0x200000
#define mask_10 0x400000
#define mask_20 0x800000
#define mask_40 0x1000000
